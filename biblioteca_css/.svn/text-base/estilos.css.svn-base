.x-form-file-wrap {
	height:22px;
	position:relative;
}

.x-form-file-wrap .x-form-file {
	height:22px;
	opacity:0;
	position:absolute;
	right:0;
	z-index:2;
}

.x-form-file-wrap .x-form-file-btn {
	position:absolute;
	right:0;
	z-index:1;
}

.x-form-file-wrap .x-form-file-text {
	color:#777777;
	left:0;
	position:absolute;
	z-index:3;
}


