body{
	background-color: #dfdecb;
	/*text-align: center;*/
	overflow: -moz-scrollbars-vertical;
	_overflow-y: auto;
	_height: 100%;
}

.clearfix{
	display: inline-block;
}

.corpo {
	background: url(../img/bg_corpo.jpg) center top no-repeat;
}

.topo {
	background: url(../img/bg_topo.jpg) center top no-repeat;
}

.direita{
	float: right;
}
.esquerda{
	float: left;
}

#wrapper {
	width:100%;
	height: 137px;
	min-width:960px;
}

#center {
	text-align: center;
	margin: 0 auto;
}

#topo{
	margin: 0 auto;
}

#corpo{
	margin: 0 auto;
}

#login{
	width: 880px;
	height: 87px;
	margin: 0 auto;
	text-align: left;
}


#menu{
	width: 870px;
	height: 29px;
	padding: 10px 0 13px 0;
}
	#menu ul{
		list-style-type: none;
	}
	
	#menu ul li{
		float: left;
		padding: 0 3px 0 0;
	}
	
	#menu h1{
		font-size: 10px;
		font-weight: normal;
		padding: 8px 0 0 0;
	}
	
	#menu h2{
		padding: 5px 0 0 0;
		font-size: 11px;
	}

#efeito{
	width: 26px; 
	height: 27px;
	overflow: hidden;
	display: list-item;
	text-indent: 10000px;
}

	#efeito:hover{
		background-position: bottom left;
	}
	
	.home{
		background: url(../img/btn_home.gif) top left no-repeat;
	}
	
	.email{
		background: url(../img/btn_email.gif) top left no-repeat;
	}
	
	.logoff{
		background: url(../img/btn_logoff.gif) top left no-repeat;
	}


#nome_sis{
	width: 870px;
	height: 30px;
	padding: 0 0 5px 0;
	font-size: 18px;
}

	#nome_sis span{
		vertical-align: 4px;
	}


#cont{
	width: 100%;
	height: 100%;
}

/* FIM CABEÇALHO */




.botaoBuscar {
  background-image:url('../img/ic_busca.png') !important;
}

.botaoSalvar {
  background-image:url('../img/ic_salvar.gif') !important;
}

.botaoEditar{
  background-image:url('../img/ic_editar.gif') !important;
}

.botaoFechar {
  background-image:url('../img/ic_deletar.gif') !important;
}

.botaoAdicionar {
  background-image:url('../img/ic_adicionar.gif') !important;
}

.botaoUpload{
  background-image:url('../img/ic_upload.png') !important;
}
 
.x-form-file-wrap {
	height:22px;
	position:relative;
}

.x-form-file-wrap .x-form-file {
	height:22px;
	opacity:0;
	position:absolute;
	right:0;
	z-index:2;
}

.x-form-file-wrap .x-form-file-btn {
	position:absolute;
	right:0;
	z-index:1;
}

.x-form-file-wrap .x-form-file-text {
	color:#777777;
	left:0;
	position:absolute;
	z-index:3;
}


